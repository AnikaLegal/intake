  {{!-- Google Analytics --}}
  {{#if GA_ID}}
  <script async src="https://www.googletagmanager.com/gtag/js?id='{{ GA_ID }}"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag() {dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', '{{ GA_ID }}');
  </script>
  {{/if}}
